{
  "name": "scatter-seed-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPL-3.0 License",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postlint": "prettier --write .",
    "lint": "cross-env eslint --ext .tsx --ext .ts .",
    "prepare": "husky install",
    "schema:download": "npx apollo client:download-schema api-schema.gql",
    "schema:gen": "npx apollo client:codegen --target typescript --localSchemaFile api-schema.gql --outputFlat src/@generated",
    "generate": "graphql-codegen --require dotenv/config --config codegen.yml",
    "postinstall": "npm run generate"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@babel/preset-env": "7.1.6",
    "@expo/metro-config": "^0.3.12",
    "@react-native-masked-view/masked-view": "0.2.6",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/material-bottom-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "expo": "^44.0.6",
    "expo-network": "^4.1.1",
    "expo-status-bar": "~1.2.0",
    "graphql": "15.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-canvas": "^0.1.38",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "2.1.0",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "2.3.1",
    "react-native-reanimated-zoom": "^0.1.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@types/react": "~17.0.21",
    "@types/react-native-canvas": "^0.1.8",
    "@types/react-native-vector-icons": "^6.4.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "apollo": "^2.33.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "expo-cli": "^5.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts . --fix"
    ]
  },
  "private": true
}
